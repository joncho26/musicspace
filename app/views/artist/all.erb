<h1 class="all-artists-header">All Artists</h1>

<div class="all-artists-section">
<ul>
<% @all_artists.each do |artist| %>
  <li><a href="/artist/<%= artist.id %>"><%= artist.name %></a></li>
<% end %>
</ul>
</div>

<div class="submit-new-artist">
  <a href="/artist/new">Don't see one you like? Click here to submit a new artist.</a>
</div>